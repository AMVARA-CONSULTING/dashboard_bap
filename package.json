{
  "name": "dip",
  "version": "2.30.0",
  "scripts": {
    "start": "ng serve --aot",
    "build": "npx json -I -f ./angular.json -e \"this.projects.dip.architect.build.options.index='src/index.html'\" && npx json -I -f ./src/manifest.json -e \"this.scope='/'\" && npx json -I -f ./src/manifest.json -e \"this.start_url='/'\" && npx ng build --prod --aot && npx json -I -f ./angular.json -e \"this.projects.dip.architect.build.options.index='src/index.html'\"",
    "build-cognos": "npx json -I -f .//angular.json -e \"this.projects.dip.architect.build.options.index='src/index.cognos.html'\" && npx json -I -f ./src/manifest.json -e \"this.scope='/internal/bi/app/DIPRE/'\" && npx json -I -f ./src/manifest.json -e \"this.start_url='/internal/bi/app/DIPRE/'\" && npx ng build --prod --aot && cd dist && rename index.cognos.html index.html && cd .. && npx json -I -f ./angular.json -e \"this.projects.dip.architect.build.options.index='src/index.html'\""
  },
  "private": true,
